                                                    "Ref": "AutomationProvider"
                                                },
                                                "-",
                                                {
                                                    "Ref": "ALIYUN::Region"
                                                }
                                            ]
                                        ]
                                    },
                                    {
                                        "Ref": "AutomationProvider"
                                    }
                                ]
                            },
                            ".oss-",
                            {
                                "Ref": "ALIYUN::Region"
                            },
                            ".aliyuncs.com/alibabacloud-quickstart/v1/sap-common/templates/Network_Single_Node.json"
